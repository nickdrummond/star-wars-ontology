@prefix : <http://null.com/star-wars#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://null.com/star-wars> .

<http://null.com/star-wars/questions> a owl:Ontology ;
                                      owl:imports <http://null.com/star-wars/all> .

###  http://null.com/star-wars#Rebel_Scum
:Rebel_Scum a owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( :Actor
                                                       [ a owl:Restriction ;
                                                         owl:onProperty :memberOf ;
                                                         owl:someValuesFrom [ a owl:Class ;
                                                                              owl:oneOf ( :Rebel_Alliance
                                                                                          :Resistance
                                                                                )
                                                           ]
                                                       ]
                                                     ) ;
                                  a owl:Class
              ] .
