#################################################################
#    Object Properties
#################################################################

###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#connectedTo
:connectedTo rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Actor
                                         :Object
                                         :Place
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Actor
                                        :Object
                                        :Place
                                      )
                        ] .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#disguisedAs
:disguisedAs rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Being
                                         :Vehicle
                                       )
                         ] ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Being
                                        :Role
                                      )
                        ] .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#fitted
:fitted rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :Equipment
                                   :Parts
                                 )
                   ] .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#gender
:gender rdfs:domain :Being ;
        rdfs:range :Gender .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#hadRole
:hadRole rdfs:domain :Being ;
         rdfs:range :Role .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#hasTerrain
:hasTerrain rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Asteroid
                                        :Moon
                                        :Planet
                                      )
                        ] ;
            rdfs:range :Habitat .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#inConflictWith
:inConflictWith rdfs:domain :Actor ;
                rdfs:range :Actor .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#inOrganisation
:inOrganisation rdfs:domain :Role ;
                rdfs:range :Organisation .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#knew
:knew rdfs:domain :Actor ;
      rdfs:range :Actor .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#locatedIn
:locatedIn rdfs:range :Place .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#madeOf
:madeOf rdfs:domain :Object ;
        rdfs:range :Material .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#ofShip
:ofShip rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Crew
                                    :Role
                                  )
                    ] ;
        rdfs:range :Ship .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#orderedBy
:orderedBy rdfs:domain :Event ;
           rdfs:range :Actor .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#ownedBy
:ownedBy rdfs:range :Actor .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#relatedTo
:relatedTo rdfs:domain :Living_thing ;
           rdfs:range :Living_thing .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#sentience
:sentience rdfs:domain :Being ;
           rdfs:range :Sentience .


###  https://raw.githubusercontent.com/nickdrummond/starwarsontology/master/ontologies#visited
:visited rdfs:range :Place .


